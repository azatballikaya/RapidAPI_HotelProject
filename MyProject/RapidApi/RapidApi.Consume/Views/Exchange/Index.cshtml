@model List<ExchangeViewModel.Exchange_Rates>
<div class="container">
    <br /><br />
    <table class="table table-bordered">
        <tr>
            <th>Döviz Kodu</th>
            <th>Birim Fiyat</th>
         
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.currency</td>
                <td>@item.exchange_rate_buy</td>
               
            </tr>
        }
    </table>
</div>